{
  "name": "vue-element-admin-vue3-vite",
  "version": "5.0.0",
  "description": "A magical vue admin based on Vue 3 and Vite. An out-of-box UI solution for enterprise applications.",
  "author": "Pan <panfree23@gmail.com> + Gemini Migration",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:prod": "vite build",
    "build:stage": "vite build --mode staging",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "echo 'Unit tests need update for Vue 3'",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "admin",
    "dashboard",
    "element-plus",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PanJiaChen/vue-element-admin.git"
  },
  "bugs": {
    "url": "https://github.com/PanJiaChen/vue-element-admin/issues"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "clipboard": "^2.0.8",
    "codemirror": "^5.65.0",
    "driver.js": "^0.9.8",
    "echarts": "^5.3.0",
    "element-plus": "^2.2.0",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.5.0",
    "js-cookie": "^3.0.0",
    "jszip": "^3.7.0",
    "moment": "^2.30.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.0",
    "screenfull": "^5.2.0",
    "sortablejs": "^1.14.0",
    "vue": "^3.2.13",
    "vue-countup-v3": "^1.4.2",
    "vue-router": "^4.0.3",
    "vue-splitpane": "^1.0.4",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.13",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "mockjs": "^1.1.0",
    "plop": "^3.0.0",
    "sass": "^1.50.0",
    "svg-sprite-loader": "6.0.9",
    "svgo": "^2.8.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^4.0.0",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">= 7.0.0"
  },
  "browserslist": [
    "defaults"
  ]
}
