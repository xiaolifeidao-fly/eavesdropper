{
  "name": "algorithm",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "algorithm",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-pc-windows-msvc"
      ]
    },
    "output": {
      "darwin-x64": {
        "name": "algorithm.darwin-x64.node"
      },
      "win32-x64-msvc": {
        "name": "algorithm.win32-x64-msvc.node"
      }
    },
    "package": {
      "name": "algorithm"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "ava": "^6.0.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "build:mac": "napi build --platform darwin-x64 --release",
    "build:win": "napi build --platform win32-x64-msvc --release",
    "build:all": "npm run build:mac && npm run setup:win-target && npm run build:win",
    "setup:win-target": "rustup target add x86_64-pc-windows-msvc",
    "clean": "rm -rf target *.node darwin-* win32-*",
    "test": "node test.js",
    "prepublishOnly": "napi prepublish -t npm",
    "universal": "napi universal",
    "version": "napi version"
  }
}
