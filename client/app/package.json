{
  "name": "eavesdropper",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "dev": "webpack --config webpack.config.js --mode development && electron .",
    "build": "webpack --config webpack.config.js --mode production",
    "start": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "package:mac": "sh package_mac.sh",
    "package:win": "sh package_win.sh"
  },
  "build": {
    "appId": "com.eavesdropper.desk.app",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [],
    "asar": true,
    "directories": {
      "output": "package"
    },
    "productName": "eavesdropper",
    "npmRebuild": true,
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns",
      "category": "com.eavesdropper.category.type"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:13500/updates/"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "deleteAppDataOnUninstall": true
    }
  },
  "_moduleAliases": {
    "@src": "dist/app/src",
    "@model": "dist/common/model",
    "@api": "dist/common/api",
    "@utils": "dist/common/utils",
    "@eleapi": "dist/common/eleapi"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/semver": "^7.5.8",
    "adm-zip": "^0.5.14",
    "asynckit": "^0.4.0",
    "axios": "^1.7.2",
    "builder-util-runtime": "^9.2.10",
    "class-transformer": "^0.5.1",
    "combined-stream": "^1.0.8",
    "crypto": "1.0.1",
    "debug": "^4.4.0",
    "dotenv": "^16.4.5",
    "electron-log": "^5.2.4",
    "electron-store": "8.1.0",
    "electron-updater": "^5.3.0",
    "follow-redirects": "^1.15.9",
    "form-data": "^4.0.1",
    "fs-extra": "^11.2.0",
    "g2@5.1.15-beta.4": "link:antv/g2@5.1.15-beta.4",
    "graceful-fs": "^4.2.11",
    "http-proxy": "^1.18.1",
    "https-proxy-agent": "^7.0.6",
    "js-yaml": "^4.1.0",
    "jsonfile": "^6.1.0",
    "lazy-val": "^1.0.5",
    "localforage": "^1.10.0",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.isequal": "^4.5.0",
    "mime-db": "^1.53.0",
    "mime-types": "^2.1.35",
    "module-alias": "^2.2.3",
    "ms": "^2.1.3",
    "playwright": "^1.49.1",
    "playwright-core": "^1.49.1",
    "proxy-from-env": "^1.1.0",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "sax": "^1.4.1",
    "semver": "^7.6.3",
    "sharp": "^0.33.5",
    "typed-emitter": "^2.1.0",
    "universalify": "^2.0.1",
    "url": "^0.11.3",
    "utils@1.13.0": "link:daybrush/utils@1.13.0",
    "uuid": "^11.0.3",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/axios": "^0.14.0",
    "@types/http-proxy": "^1.17.14",
    "@types/node": "^20.14.5",
    "@types/qs": "^6.9.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^31.7.6",
    "electron-builder": "^24.13.3",
    "file-loader": "^6.2.0",
    "node-loader": "^2.1.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.5.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-node-externals": "^3.0.0"
  }
}
