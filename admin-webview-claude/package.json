{
  "name": "vue-element-admin",
  "version": "4.2.1",
  "description": "A magical vue admin. An out-of-box UI solution for enterprise applications. Newest development stack of vue. Lots of awesome features",
  "author": "Pan <panfree23@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:prod": "vite build",
    "build:stage": "vite build --mode staging",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "vitest",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop",
    "start": "pm2 start server.js --name 'my-app'",
    "stop": "pm2 stop my-app",
    "restart": "pm2 restart my-app",
    "logs": "pm2 logs my-app",
    "delete": "pm2 delete my-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-plus",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PanJiaChen/vue-element-admin.git"
  },
  "bugs": {
    "url": "https://github.com/PanJiaChen/vue-element-admin/issues"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.6.2",
    "camelcase": "^7.0.1",
    "clipboard": "^2.0.11",
    "codemirror": "^6.0.1",
    "cross-spawn": "^7.0.3",
    "driver.js": "^1.3.1",
    "dropzone": "^6.0.0-beta.2",
    "echarts": "^5.4.3",
    "element-plus": "^2.9.7",
    "file-saver": "^2.0.5",
    "fuse.js": "^7.0.0",
    "glob": "^11.0.1",
    "js-cookie": "^3.0.5",
    "jsonlint": "^1.6.3",
    "jszip": "^3.10.1",
    "moment": "^2.30.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.1.7",
    "pm2": "^5.3.0",
    "sass": "^1.69.5",
    "screenfull": "^6.0.2",
    "showdown": "^2.1.0",
    "sortablejs": "^1.15.1",
    "vue": "^3.3.9",
    "vue-count-to": "^1.0.13",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/register": "^7.22.15",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.16",
    "babel-eslint": "^10.1.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "connect": "^3.7.0",
    "eslint": "^8.55.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "mockjs": "^1.1.0",
    "plop": "^4.0.0",
    "postcss": "^8.4.32",
    "sass": "^1.69.5",
    "serve-static": "^1.15.0",
    "unplugin-vue-define-options": "^3.0.0-beta.8",
    "vite": "^5.0.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">= 8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
